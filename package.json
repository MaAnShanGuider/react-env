{
  "name": "react-env-pc",
  "version": "0.1.0",
  "description": "basic env for pc",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run prompt && npm run all",
    "serve": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --color --progress --hot",
    "dist": "npm run remove:dist && npm run lint && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js --color --progress",
    "build": "npm run dist -- -p",
    "build:dev": "npm run dist",
    "remove:dist": "rm -rf ./dist",
    "lint": "esw ./config/*.config.* ./src/pages",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "mock": "cd ./src/mock && npm run start",
    "prompt": "node ./config/prompt.config.js",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "postinstall": "cp .hooks/* .git/hooks/"
  },
  "author": "wya-team",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.0.1",
    "antd-mobile": "^2.1.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "lrz": "^4.9.40",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fastclick": "^3.0.2",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.0.8",
    "postcss-smart-import": "^0.7.5",
    "precss": "^2.0.0",
    "prompt": "^1.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
